{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts", "coverage/lcov-report/*.js", "./src/server/**/*.js"]
  },
  "rules": {
    "member-access": false,
    "jsx-boolean-value": [true, "never"],
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "no-console": {
      "severity": "warning"
    }
  }
}
